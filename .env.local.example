# One Path (GoHighLevel) API Configuration
# Get these from your GoHighLevel account
ONEPATH_API_KEY="your-api-key-here"
ONEPATH_LOCATION_ID="your-location-id-here"

# Stripe Configuration
# Get these from your Stripe Dashboard (https://dashboard.stripe.com/apikeys)

# Publishable Key - SAFE to expose in browser (uses VITE_ prefix)
# This key is DESIGNED to be public and can only create checkout sessions
# It cannot charge cards or access sensitive data
VITE_STRIPE_PUBLISHABLE_KEY="pk_test_your-publishable-key-here"

# Secret Key - NEVER expose in browser (NO VITE_ prefix)
# This key can charge cards and MUST stay server-side only
# Used only in api/create-checkout.ts serverless function
STRIPE_SECRET_KEY="sk_test_your-secret-key-here"

# Stripe Price IDs for each micro-offer (NO VITE_ prefix - server-side only)
# Create these in Stripe Dashboard â†’ Products
# These are used in the serverless function, not the browser

# UNIFIED PRODUCT - The Ultimate Growth Architect at $7 (RECOMMENDED)
# This single price is used for all pillars
STRIPE_PRICE_ULTIMATE="price_ultimate_id_here"

# Legacy individual prices (optional - fallback if STRIPE_PRICE_ULTIMATE not set)
STRIPE_PRICE_FOUNDATION="price_foundation_id_here"
STRIPE_PRICE_ARCHITECTURE="price_architecture_id_here"
STRIPE_PRICE_BUILD="price_build_id_here"
STRIPE_PRICE_RELEASE="price_release_id_here"
STRIPE_PRICE_IMPROVE="price_improve_id_here"
STRIPE_PRICE_COMPOUND="price_compound_id_here"
